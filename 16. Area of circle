

#include<stdio.h>

int main()
{
    float radis,area;

    printf("Enter the circle radis value: ");
    scanf("%f",&radis);


    area = 3.1416* radis*radis;

    printf("Area of Circle = %.2f",area);

    return 0;



}

#include<stdio.h>
#include<math.h.>

int main()
{
    float radis,area;

    printf("Enter the circle radis value: ");
    scanf("%f",&radis);


    area = M_PI* radis*radis;

    printf("Area of Circle = %.2f",area);

    return 0;



}
