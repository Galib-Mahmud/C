
#include<stdio.h>
int main()
{

int n,row,col;
printf("Enter N : ");
scanf("%d",&n);
for(row=1;row<=n;row++)
{
    for(col=1;col<=row;col++)
    {
        printf("%d  ",col);

    }
    printf("\n");
}



}


#include<stdio.h>
int main()
{

int n,row,col;
printf("Enter N : ");
scanf("%d",&n);
for(row=1;row<=n;row++)
{
    for(col=1;col<=row;col++)
    {
        printf("%d  ",row);

    }
    printf("\n");
}



}



#include<stdio.h>
int main()
{

int n,row,col;
printf("Enter N : ");
scanf("%d",&n);
for(row=1;row<=n;row++)
{
    for(col=1;col<=row;col++)
    {
        printf("%d  ",col%2);

    }
    printf("\n");
}



}




#include<stdio.h>
int main()
{

int n,row,col;
printf("Enter N : ");
scanf("%d",&n);
for(row=1;row<=n;row++)
{
    for(col=1;col<=row;col++)
    {
        printf("%d  ",row%2);

    }
    printf("\n");
}



}


//charecter 

#include<stdio.h>
int main()
{

int n,row,col;
printf("Enter N : ");
scanf("%d",&n);
for(row=1;row<=n;row++)
{
    for(col=1;col<=row;col++)
    {
        printf("%c  ",col+64);//96 for small

    }
    printf("\n");
}



}
