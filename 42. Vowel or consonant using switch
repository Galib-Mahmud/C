#include<stdio.h>
int main()
{
    //switch case break default
    int ch;
    printf("Enter number : ");
    scanf("%d",&ch);
    switch(ch)
    {
    case 'a':
    case 'e':
    case 'i':
    case 'o':
    case 'u':
    case 'A':
    case 'E':
    case 'I':
    case 'O':
    case 'U':

        printf("Vowel\n");
        break;

    default:
        printf("Consonant");


        return 0;
    }
