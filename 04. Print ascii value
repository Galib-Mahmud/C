#include<stdio.h>
int main()
{

   int i;
   float f;
   char c;
   double d;
   printf("size of int=%d bytes\n",sizeof(i));
       printf("size of float=%d bytes\n",sizeof(f));
          printf("size of char=%d bytes\n",sizeof(c));
             printf("size of double=%d bytes\n",sizeof(d));

    return 0;
}
