#include<stdio.h>
int main()
{
   int array1[30],n,array2[32],i;
   printf("How many numbers:");
   scanf("%d",&n);
   for(i=0;i<n;i++)
   {
       scanf("%d",&array1[i]);
   }
    printf("array1 : ");
    for(i=0;i<5;i++)
    {
        printf("%d ",array1[i]);
    }

    for(i=0;i<5;i++)
    {
        array2[i]=array1[i];
    }
    printf("\nArray2 : ");
    for(i=0;i<5;i++)
    {
        printf("%d ",array2[i]);
    }
    getch();
}
