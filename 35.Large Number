
#include<stdio.h>
main()
{
    int num1,num2;
    printf("Enter two number ");
    scanf("%d %d",&num1,&num2);
    if(num1>num2)
    {
        printf("The large number is= %d\n",num1);

    }
    else if(num1<num2)

    {
        printf("The large number is= %d",num2);

    }
    else
    {

        printf("The number is Equal");
    }
    getch();
}
