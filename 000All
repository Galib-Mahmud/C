29.Increment Decrement 

#include<stdio.h>
main()
{
int x=10;
int y=x++;
printf("x=%d\n",x);
printf("y=%d\n",y);
return 0;

}



#include<stdio.h>
main()
{
int x=10;
int y=--x;
printf("x=%d\n",x);
printf("y=%d\n",y);
return 0;

}


