55.Reverse





#include<stdio.h>
int main()
{
int i,temp,rem,sum=0,num;

printf("Enter any number: ");
scanf("%d",&num);
temp=num;
while(temp!=0)
{
    rem=temp%10;
    sum=sum*10+rem;
    temp=temp/10;
}
printf("Reverse of  :%d\n",sum);
getch();

}



