90. Pointer swaping 



#include <stdio.h>
int main()
{
    int x=10,y=20,temp;
   int *ptr1,*ptr2;
   ptr1 =&x;
   ptr2 =&y;
  //swaping
  temp= *ptr1;
  *ptr1= *ptr2;
  *ptr2= temp;

   printf("x = %d\n",x);
   printf("y = %d\n",y);
   getch();
}


