59.Strong Number


#include<stdio.h>
int main()
{
int i, temp,rem,sum=0,num,fact;

printf("Enter any number: ");
scanf("%d",&num);
temp=num;
while(temp!=0)
{
    rem=temp%10;
    fact=1;
    for(i=1;i<=rem;i++)
    {
        fact=fact*i;

    }
    sum=sum+fact;
    temp=temp/10;
}
if(sum==num)
    printf("%d is a Strong Number",num);
else
    printf("%d is a Strong Number",num);
getch();

}
