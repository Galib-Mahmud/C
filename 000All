51.Factorial 



#include<stdio.h>
int main()
{

  int num,i,fact=1;
        printf("Enter any number : ");
        scanf("%d",&num);
        for(i=1;i<=num;i++)
            fact=fact*i;
        printf("%d\n",fact);
        return 0;

}


