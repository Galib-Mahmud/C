57.Armstrong



#include<stdio.h>
int main()
{
int i,temp,rem,sum=0,num;

printf("Enter any number: ");
scanf("%d",&num);
temp=num;
while(temp!=0)
{
    rem=temp%10;
    sum=sum +rem*rem*rem;
    temp=temp/10;
}
if(num==sum)
    printf("Armstrong");
else
    printf("Not Armstrong");
getch();

}


