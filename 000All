14. given three base of a triangle



#include<stdio.h>
#include<math.h>
int main()
{
    double num1,num2,num3,s,area;

    printf("Enter three value: ");
    scanf("%lf %lf %lf",&num1,&num2,&num3);
    s = (num1+num2+num3)/2 ;
    area = sqrt(s*(s-num1)*(s-num2)*(s-num3));

    printf("Area of %.2lf",area);

    return 0;



}



