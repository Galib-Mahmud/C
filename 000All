74.Addition of Matrix



#include<stdio.h>
int main(){
int i,j,NumberOfRows,NumberOfCols;
int A[10][10],B[10][10],C[10][10];
printf("Enter Rows And Cols:");
scanf("%d %d",&NumberOfRows,&NumberOfCols);

//Scaning A
printf("Enter elements of a matrix :\n");
for(i=0;i<NumberOfRows;i++)
{
    for(j=0;j<NumberOfCols;j++)
    {
        printf("A[%d][%d]=",i,j);
        scanf("%d",&A[i][j]);
    }
    printf("\n");
}

//Scaning B
printf("Enter elements of b matrix :\n");
for(i=0;i<NumberOfRows;i++)
{
    for(j=0;j<NumberOfCols;j++)
    {
        printf("B[%d][%d]=",i,j);
        scanf("%d",&B[i][j]);
    }
    printf("\n");
}
//Printing A
printf("A = ");
for(i=0;i<NumberOfRows;i++)
{
    printf("\t");
    for(j=0;j<NumberOfCols;j++)
    {
        printf("%d   ",A[i][j]);
    }
    printf("\n");
}


//Printing B
printf("B = ");
for(i=0;i<NumberOfRows;i++)
{
    printf("\t");
    for(j=0;j<NumberOfCols;j++)
    {
        printf("%d   ",B[i][j]);
    }
    printf("\n");
}

//Adding the mattrix

for(i=0;i<NumberOfRows;i++)
{
    for(j=0;j<NumberOfCols;j++)
    {
      C[i][j] =A[i][j] + B[i][j];
    }


}
  //printing C matrix

  printf(" A+B = ");
  for(i=0;i<NumberOfRows;i++)
{
    printf("\t");
    for(j=0;j<NumberOfCols;j++)
    {
        printf("%d   ",C[i][j]);
    }
    printf("\n");
}

}


